{
  "name": "hackerai",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.30.2",
  "scripts": {
    "dev": "concurrently \"next dev --turbopack\" \"npx convex dev\"",
    "dev:next": "next dev --turbopack",
    "dev:convex": "npx convex dev",
    "dev:trigger": "npx trigger.dev@latest dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint app lib types __mocks__ --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup": "pnpm install && npx tsx scripts/setup.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:report": "playwright show-report",
    "rate-limit:reset": "npx tsx scripts/reset-rate-limit.ts",
    "stripe:attach-failing-card": "npx tsx scripts/attach-failing-card.ts",
    "prepare": "husky",
    "s3:validate": "npx ts-node scripts/validate-s3-security.ts",
    "e2b:build:dev": "npx tsx e2b/build.dev.ts",
    "e2b:build:prod": "npx tsx e2b/build.prod.ts",
    "local-sandbox": "npx tsx packages/local/src/index.ts",
    "local-sandbox:build": "cd packages/local && pnpm build",
    "desktop:dev": "cd packages/desktop && pnpm dev",
    "desktop:build": "cd packages/desktop && pnpm build"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.31",
    "@ai-sdk/openai-compatible": "^1.0.33",
    "@ai-sdk/react": "^3.0.99",
    "@ai-sdk/xai": "^3.0.57",
    "@aws-sdk/client-s3": "^3.996.0",
    "@aws-sdk/s3-request-presigner": "^3.996.0",
    "@axiomhq/js": "^1.4.0",
    "@axiomhq/logging": "^0.2.0",
    "@axiomhq/nextjs": "^0.2.0",
    "@axiomhq/react": "^0.2.0",
    "@convex-dev/aggregate": "^0.2.1",
    "@e2b/code-interpreter": "2.3.3",
    "@langchain/community": "^1.1.18",
    "@monaco-editor/react": "^4.7.0",
    "@openrouter/ai-sdk-provider": "^2.2.3",
    "@posthog/ai": "^7.9.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@trigger.dev/react-hooks": "4.4.1",
    "@trigger.dev/sdk": "4.4.1",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "@vercel/functions": "^3.4.2",
    "ai": "^6.0.97",
    "ai-elements": "^1.8.4",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.32.0",
    "date-fns": "^4.1.0",
    "e2b": "^2.12.1",
    "gpt-tokenizer": "^3.4.0",
    "iron-session": "^8.0.4",
    "isbinaryfile": "^6.0.0",
    "jszip": "^3.10.1",
    "langchain": "^1.2.26",
    "lucide-react": "^0.575.0",
    "mammoth": "^1.11.0",
    "marked": "^17.0.3",
    "motion": "^12.34.3",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "openai": "^6.23.0",
    "pdfjs-serverless": "^1.1.0",
    "posthog-js": "^1.353.0",
    "posthog-node": "^5.25.0",
    "react": "^19.2.4",
    "react-dom": "19.2.4",
    "react-hotkeys-hook": "^5.2.4",
    "react-shiki": "^0.9.1",
    "react-textarea-autosize": "^8.5.9",
    "redis": "^5.11.0",
    "resumable-stream": "^2.2.10",
    "shiki": "^3.22.0",
    "sonner": "^2.0.7",
    "streamdown": "^2.3.0",
    "stripe": "^20.3.1",
    "tailwind-merge": "^3.5.0",
    "use-stick-to-bottom": "^1.1.3",
    "uuid": "^13.0.0",
    "word-extractor": "^1.0.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4",
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@trigger.dev/build": "4.4.1",
    "@types/jest": "^30.0.0",
    "@types/jszip": "^3.4.1",
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/word-extractor": "^1.0.6",
    "concurrently": "^9.2.1",
    "dotenv": "^17.3.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "axios": "1.13.5",
      "glob@>=10.0.0 <10.5.0": "10.5.0",
      "glob@>=11.0.0 <11.1.0": "11.1.0",
      "js-yaml": "4.1.1",
      "mdast-util-to-hast": "13.2.1",
      "vite": "7.1.11",
      "tar": "7.5.9",
      "@isaacs/brace-expansion": "5.0.1",
      "diff@>=4.0.0 <4.0.4": "4.0.4",
      "diff@>=6.0.0 <8.0.3": "8.0.3",
      "ajv@>=6.0.0 <6.14.0": "6.14.0"
    }
  }
}
